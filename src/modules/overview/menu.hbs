---
src:
  headline:
    text: 'src'
  list:
    'Component 1': 'docs/elements/link/menu.html'
    'Component 2':
      'Component 2.1': 'docs/elements/link/menu.html'
      'Component 2.2': '#component-2-2'
      'Component 2.3':
        'Component 2.3.1': '#component-1-3-1'
        'Component 2.3.2': '#component-1-3-2'
        'Component 2.3.3': '#component-1-3-3'
---
<div class="module controller" data-module="gp-boilerplate-documentation/modules/overview/menu" data-controller="gp-boilerplate-documentation/modules/overview/Menu" data-chunk="gp-boilerplate-documentation" data-target="[data-module='gp-boilerplate-documentation/modules/overview']">
  <span class="menu__toggle" data-hook="menuToggle"><i></i></span>
  <div>
    <div class="menu__scroll-wrapper">
      <div class="menu__content">
          {{#block "content"}}

            <div class="src">
              {{{mixin "gp-boilerplate-documentation/modules/elements/headline" src.headline}}}
              {{#each src.list}}
                {{{mixin "gp-boilerplate-documentation/modules/elements/accordeon" this parent-id="agency-src"}}}
              {{/each}}
            </div>

          {{/block}}
      </div>
    </div>
  </div>
</div>
